						path.st.st--0(d="M58 108h14V74L52 59v43c0 3.32 2.69 6 6 6" fill="currentColor")
						path.st.st--1(d="M120 108h14c3.32 0 6-2.69 6-6V59l-20 15" fill="currentColor")
						path.st.st--2(d="M120 48v26l20-15v-8c0-7.42-8.47-11.65-14.4-7.2" fill="currentColor")
						path.st.st--3(d="M72 74V48l24 18 24-18v26L96 92" fill="currentColor")
						path.st.st--4(d="M52 51v8l20 15V48l-5.6-4.2c-5.94-4.45-14.4-.22-14.4 7.2" fill="currentColor")
					//- +icon("icon-gmail", 100, 100).cv-contacts__icon.cv-contacts__icon--gmail
				a.cv-contacts__link.cv-contacts__link--phone.link(href="tel:+79046121580")
					+icon("icon-iphone", 100, 100).cv-contacts__icon.cv-contacts__icon--phone
				a.cv-contacts__link.cv-contacts__link--linkedin.link(href="https://www.linkedin.com/in/%D1%8F%D1%80%D0%BE%D1%81%D0%BB%D0%B0%D0%B2-%D0%BA%D0%BE%D0%B2%D0%B0%D0%BB%D0%B5%D0%B2-101b42165" target="_blank" rel="noopener noreferrer nofollow")
