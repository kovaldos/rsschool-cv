				a.cv-contacts__link.cv-contacts__link--discord.link(href="https://discord.com/users/kovaldos#1827" target="_blank" rel="noopener noreferrer nofollow")
					+icon("icon-discord", 100, 100).cv-contacts__icon.cv-contacts__icon--discord
