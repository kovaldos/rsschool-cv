mixin cv-contacts ()
	-
		const data = {
			title: "Contact me",
			items: [
				{
					href: "mailto:orblack2@gmail.com",
					icon: "icon-js",
					linkClass: "cv-skills__link--js",
					iconClass: "cv-skills__icon--js",
				},
			],
			email: "orblack2@gmail.com",
			tel: "+7(904)612-15-80",
			tg: "@kovaldos",
			dscrd: "@kovaldos",
